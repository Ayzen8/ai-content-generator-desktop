{
  "name": "ai-content-generator-web",
  "version": "1.0.0",
  "description": "Web application for AI Content Generator - Create engaging content for social media",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "npm run build:react",
    "build:react": "webpack --mode production",
    "vercel-build": "npm run build",
    "dev": "concurrently \"npm run build\" \"npm run start\"",
    "dev:watch": "concurrently \"npm run watch:react\" \"npm run start\"",
    "watch:react": "webpack --mode development --watch",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "axios": "^1.10.0",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "form-data": "^4.0.3",
    "fs-extra": "^11.3.0",
    "multer": "^2.0.1",
    "oauth": "^0.10.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sqlite3": "^5.1.7",
    "twitter-api-v2": "^1.23.2",
    "webpack": "^5.99.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "css-loader": "^6.8.1",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack-cli": "^5.1.4"
  }
}
